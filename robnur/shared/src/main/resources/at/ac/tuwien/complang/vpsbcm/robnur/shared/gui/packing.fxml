            </columns>
        </TableView>

        <TableView fx:id="tvFlowers">
            <columns>
                <TableColumn fx:id="tcFlowersId" prefWidth="320" text="Id" />
                <TableColumn fx:id="tcFlowersType" prefWidth="100" text="Typ" />
                <TableColumn fx:id="tcFlowerParent" text="Pflanzen Id" prefWidth="320"/>
