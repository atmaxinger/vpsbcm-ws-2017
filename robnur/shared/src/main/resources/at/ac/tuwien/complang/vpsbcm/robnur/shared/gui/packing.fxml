<?import javafx.scene.layout.*?>

<SplitPane prefHeight="369.0" prefWidth="504.0" dividerPositions="0.66" orientation="VERTICAL"
           xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.complang.vpsbcm.robnur.shared.gui.PackagingController">
    <items>
        <TableView fx:id="tvVegetables">
            <columns>
                <TableColumn fx:id="tcVegetablesId" prefWidth="320" text="Id" />
                <TableColumn fx:id="tcVegetablesType" prefWidth="100" text="Typ" />
                <TableColumn fx:id="tcVegetablesParent" text="Pflanzen Id" prefWidth="320"/>
                <TableColumn fx:id="tcVegetablesPlantedBy" text="Pflanz Roboter" prefWidth="100" />
